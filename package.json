{
  "name": "ace",
  "version": "1.1.0",
  "description": "This is an enternaining application built with VueJS to play the popular french card game Coinche",
  "main": "dist/index.html",
  "scripts": {
    "webpack-server": "./node_modules/.bin/webpack-dev-server -d",
    "webpack": "./node_modules/.bin/webpack",
    "build": "export NODE_ENV=production && npm run webpack -- -p",
    "watch": "npm run webpack -- -w",
    "api": "./node_modules/.bin/nodemon dist/api/api.js",
    "watch-api": "npm run watch -- --config api/webpack.config.js"
  },
  "dependencies": {
    "add-to-homescreen": "^3.2.6",
    "crypto-js": "^3.1.9-1",
    "cube-ui": "^1.9.9",
    "deck-of-cards": "^0.1.8",
    "flag-icon-css": "^3.0.0",
    "font-awesome": "^4.7.0",
    "inobounce": "^0.1.5",
    "interactjs": "^1.3.4",
    "vue": "^2.3.3",
    "vue-browserstore": "^0.1.4",
    "vue-resource": "^1.3.5",
    "vue-router": "^3.0.1",
    "vue-save-state": "^1.2.0",
    "vue-simple-markdown": "^1.0.6",
    "vue-touch": "^2.0.0-beta.4",
    "vuex": "^2.3.1",
    "vuex-i18n": "^1.10.4",
    "vuex-local-state": "^1.1.2"
  },
  "devDependencies": {
    "appcache-webpack-plugin": "^1.3.0",
    "argparce": "^1.0.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.14.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.16.1",
    "eslint-config-google": "^0.7.1",
    "eslint-config-vue": "^2.0.2",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-vue": "^2.0.1",
    "express": "^4.16.3",
    "favicons-webpack-plugin": "0.0.9",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "json-loader": "^0.5.7",
    "node-sass": "^4.9.1",
    "nodemon": "^1.17.5",
    "object.assign": "^4.1.0",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^11.3.4",
    "vue-template-compiler": "^2.1.10",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.4.5",
    "webpack-transform-modules-plugin": "^0.3.1"
  },
  "transformModules": {
    "cube-ui": {
      "transform": "cube-ui/lib/${member}",
      "kebabCase": true,
      "style": {
        "ignore": [
          "create-api",
          "better-scroll"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NikolaLohinski/ace.git"
  },
  "keywords": [
    "awesome",
    "coinche",
    "cards",
    "ia"
  ],
  "author": "N.Lohinski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/NikolaLohinski/ace/issues"
  },
  "homepage": "https://github.com/NikolaLohinski/ace#readme"
}
